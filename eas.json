{
  "cli": {
    "version": ">= 0.48.1"
  },
  "build": {
    "dev": {
      "env": {
        "ENV": "dev"
      },
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "env": {
        "ENV": "preview"
      },
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "env": {
        "ENV": "production"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "red": {
      "extends": "production",
      "env": {
        "BUILDCONFIG": "red"
      }
    },
    "blue": {
      "extends": "production",
      "env": {
        "BUILDCONFIG": "blue"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
